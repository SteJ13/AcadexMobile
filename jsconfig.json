{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@context/*": [
                "src/context/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@screens/*": [
                "src/screens/*"
            ],
            "@navigation/*": [
                "src/navigation/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ],
            "@notifications/*": [
                "src/notifications/*"
            ],
            "@config/*": [
                "src/config/*"
            ],
            "@constants/*": [
                "src/constants/*"
            ],
            "@translate/*": [
                "src/translate/*"
            ]
        }
    },
    "exclude": [
        "node_modules",
        "babel.config.js"
    ]
}